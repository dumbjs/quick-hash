{
  "name": "@dumbjs/quick-hash",
  "version": "0.0.0",
  "description": "Quick non-cryptographic hash functions for strings",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "default": "./dist/esm/index.js"
    },
    "./djb2": {
      "require": "./dist/cjs/djb2.js",
      "import": "./dist/esm/djb2.js",
      "default": "./dist/esm/djb2.js"
    },
    "./sdbm": {
      "require": "./dist/cjs/sdbm.js",
      "import": "./dist/esm/sdbm.js",
      "default": "./dist/esm/sdbm.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "rollup -c",
    "next": "bumpp",
    "test": "uvu",
    "test:ci": "c8 npm run test"
  },
  "prettier": "@barelyhuman/prettier-config",
  "license": "MIT",
  "devDependencies": {
    "@barelyhuman/prettier-config": "^1.1.0",
    "bumpp": "^9.2.0",
    "c8": "^8.0.1",
    "prettier": "^3.0.3",
    "rollup": "^4.4.0",
    "rollup-node-externals": "0.0.1-2",
    "rollup-plugin-node-externals": "^6.1.2",
    "uvu": "^0.5.6"
  },
  "dependencies": {
    "@barelyhuman/astring-jsx": "^2.0.1",
    "acorn": "^8.11.2",
    "acorn-class-fields": "^1.0.0",
    "acorn-import-assertions": "^1.9.0",
    "acorn-jsx": "^5.3.2",
    "acorn-jsx-walk": "^2.0.0",
    "acorn-static-class-features": "^1.0.0",
    "acorn-walk": "^8.3.0",
    "sucrase": "^3.34.0"
  }
}
